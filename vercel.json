{
  "version": 2,
  "builds": [
    {
      "src": "api/dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/dist/main.js"
    }
  ],
  "buildCommand": "pnpm build:web && pnpm --filter api build",
  "outputDirectory": "api/dist",
  "installCommand": "pnpm install",
  "startCommand": "pnpm run start:prod"
}
